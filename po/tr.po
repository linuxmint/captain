msgstr[0] "%d diğer pakette değişiklik gerektiriyor"
msgstr[1] "%d diğer pakette değişiklik gerektiriyor"

#: usr/lib/captain/captain.py:263
msgid "KB"
msgstr "KB"

#: usr/lib/captain/captain.py:263
msgid "MB"
msgstr "MB"

#: usr/lib/captain/captain.py:263 usr/lib/captain/captain.py:264
msgid "GB"
msgstr "GB"

