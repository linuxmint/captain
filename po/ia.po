#: usr/lib/captain/captain.py:263
msgid "KB"
msgstr "kB"

#: usr/lib/captain/captain.py:263
msgid "MB"
msgstr "MB"

#: usr/lib/captain/captain.py:263 usr/lib/captain/captain.py:264
msgid "GB"
msgstr "GB"

#: usr/lib/captain/captain.py:299
msgid "It is safer and recommended to install from the repositories instead."
